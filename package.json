{
  "name": "apadana",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "src/scripts/start-local-db.sh && pnpm run migrate:dev",
    "dev": "dotenv -e .env.local -- concurrently --prefix-colors \"blue.bold,magenta.bold,green.bold,yellow.bold\" npm:dev:*",
    "dev:next": "next dev",
    "dev:prisma": "prisma generate --watch",
    "dev:storybook": "storybook dev -p 6006 --no-open --quiet",
    "dev:smee": "node src/scripts/smee-client.js",
    "migrate:dev": "dotenv -e .env.local prisma migrate dev",
    "drop-local-db": "src/scripts/delete-local-db-data.sh",
    "prisma:generate": "prisma generate",
    "prisma:watch": "prisma generate --watch",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "pnpm prisma:generate && tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "e2e": "playwright test --project=chromium",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@clerk/nextjs": "^5.3.7",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
