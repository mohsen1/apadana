{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "src/scripts/build-vercel.sh",
  "headers": [
    {
      "source": "/fonts/inter-var-latin.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "preview": true
    }
  }
}
